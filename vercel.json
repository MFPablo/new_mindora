{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/api/index.ts"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/server/api/index.ts"
    },
    {
      "source": "/((?!api|auth|assets|favicon.ico|vite.svg).*)",
      "destination": "/index.html"
    }
  ]
}